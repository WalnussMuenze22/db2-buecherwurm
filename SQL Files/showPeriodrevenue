CREATE OR REPLACE  FUNCTION showPeriodRevenue(startDatum IN DATE, endDatum IN DATE, umsatz OUT Double)
IS

        BEGIN
            for zeile IN (
                SELECT  GESAMTPREIS FROM BESTELLUNG WHERE (Datum >= startDatum AND Datum <= endDatum)
                )
            LOOP
                umsatz = umsatz + zeile.GESAMTPREIS;
                end loop;
            return umsatz;
        end;